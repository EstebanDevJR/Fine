APP_ENV=local
SECRET_KEY=change-me
# CORS (local dev defaults). For Vercel/DigitalOcean set this to your deployed frontend URL(s).
# Example:
# CORS_ORIGINS=https://your-frontend.vercel.app,http://localhost:5173
CORS_ORIGINS=http://localhost:5173,http://localhost:3000
# If you ever switch from Bearer tokens to cookie-based auth, set this true (and do NOT use wildcard origins).
CORS_ALLOW_CREDENTIALS=false
# Postgres (async): use asyncpg driver. Example Supabase:
# DATABASE_URL=postgresql+asyncpg://postgres:<PASSWORD>@db.<hash>.supabase.co:5432/postgres?sslmode=require
DATABASE_URL=sqlite+aiosqlite:///./data/app.db
DATA_PATH=./data
REDIS_URL=redis://redis:6379/0
OPENAI_API_KEY=sk-...
LOG_LEVEL=INFO
STORAGE_BASE_PATH=/app/app/storage
REPORTS_PATH=/app/app/storage/reports
MODELS_PATH=/app/app/storage/models
DATASETS_PATH=/app/app/storage/datasets
ARTIFACTS_PATH=/app/app/storage/artifacts
# Supabase Auth
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_JWT_SECRET=
SUPABASE_JWKS_URL=
# AWS S3
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=
S3_BUCKET_DATASETS=
S3_BUCKET_MODELS=
S3_BUCKET_REPORTS=
S3_BUCKET_ARTIFACTS=

